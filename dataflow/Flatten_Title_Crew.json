{
	"name": "Flatten_Title_Crew",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "TitleCrewCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable2",
						"type": "DatasetReference"
					},
					"name": "ToSnowflake"
				}
			],
			"transformations": [
				{
					"name": "FlattenTitleCrewDirectors"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "FlattenWriters"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Job_ID as string (\"10\"),",
				"     Load_DT as date (currentDate())",
				"}",
				"source(output(",
				"          tconst as string,",
				"          directors as string,",
				"          writers as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> TitleCrewCSV",
				"derivedColumn1 foldDown(unroll(directors),",
				"     mapColumn(",
				"          directors = directors,",
				"          tconst,",
				"          writers",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> FlattenTitleCrewDirectors",
				"TitleCrewCSV derive(directors = split(directors, ','),",
				"          writers = split(writers,',')) ~> derivedColumn1",
				"FlattenTitleCrewDirectors foldDown(unroll(writers),",
				"     mapColumn(",
				"          directors,",
				"          tconst,",
				"          writers",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> FlattenWriters",
				"FlattenWriters derive(DI_LOAD_DT = $Load_DT,",
				"          DI_JOB_ID = $Job_ID) ~> derivedColumn2",
				"derivedColumn2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TCONST as string,",
				"          DIRECTORS as string,",
				"          WRITERS as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          TCONST = tconst,",
				"          DIRECTORS = directors,",
				"          WRITERS = writers,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DATE = DI_LOAD_DT",
				"     )) ~> ToSnowflake"
			]
		}
	}
}