{
	"name": "DimRegionLoad",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable5",
						"type": "DatasetReference"
					},
					"name": "sourceCountryCode"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable6",
						"type": "DatasetReference"
					},
					"name": "sinkDimRegion"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     di_job_id as string (\"100\"),",
				"     di_load_date as date (currentDate())",
				"}",
				"source(output(",
				"          CODE as string,",
				"          COUNTRY as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourceCountryCode",
				"sourceCountryCode keyGenerate(output(RegionSK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(DI_JOB_ID = $di_job_id,",
				"          DI_LOAD_DATE = $di_load_date) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          REGION_SK as string,",
				"          COUNTRY_NAME as string,",
				"          COUNTRY_CODE as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          REGION_SK = RegionSK,",
				"          COUNTRY_NAME = COUNTRY,",
				"          COUNTRY_CODE = CODE,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DATE",
				"     )) ~> sinkDimRegion"
			]
		}
	}
}