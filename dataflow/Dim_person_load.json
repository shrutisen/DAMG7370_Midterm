{
	"name": "Dim_person_load",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable11",
						"type": "DatasetReference"
					},
					"name": "LoadFromSnowflake"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeTable12",
						"type": "DatasetReference"
					},
					"name": "MaxPersonSK"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeTable13",
						"type": "DatasetReference"
					},
					"name": "SourcePersonDim"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable10",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "SourceDataWithSK"
				},
				{
					"name": "select1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "joinToTarget"
				},
				{
					"name": "filter1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Job_ID as string (\"69\")",
				"}",
				"source(output(",
				"          NCONST as string,",
				"          PRIMARYNAME as string,",
				"          PRIMARYPROFESSION as string,",
				"          KNOWNFORTITLES as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date,",
				"          BIRTHYEAR as string,",
				"          DEATHYEAR as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> LoadFromSnowflake",
				"source(output(",
				"          MAXPERSONSK as decimal(38,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT NVL(MAX(Person_SK),0) AS MaxPersonSK FROM DAMG_IMDB.DIM_PERSON',",
				"     format: 'query') ~> MaxPersonSK",
				"source(output(",
				"          PERSON_SK as decimal(38,0),",
				"          NCONST as string,",
				"          PRIMARYNAME as string,",
				"          BIRTHYEAR as string,",
				"          DEATHYEAR as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> SourcePersonDim",
				"surrogateKey1 derive(DI_JOB_ID = $Job_ID,",
				"          DI_LOAD_DATE = currentDate(),",
				"          Person_SK = MAXPERSONSK + RowNum) ~> derivedColumn1",
				"LoadFromSnowflake, MaxPersonSK join(1==1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> SourceDataWithSK",
				"SourceDataWithSK select(mapColumn(",
				"          NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          MAXPERSONSK",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 aggregate(groupBy(NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          MAXPERSONSK),",
				"     COUNT_ROW = count(NCONST)) ~> aggregate1",
				"aggregate1, SourcePersonDim join(aggregate1@NCONST == SourcePersonDim@NCONST",
				"     && aggregate1@PRIMARYNAME == SourcePersonDim@PRIMARYNAME",
				"     && aggregate1@BIRTHYEAR == SourcePersonDim@BIRTHYEAR",
				"     && aggregate1@DEATHYEAR == SourcePersonDim@DEATHYEAR,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinToTarget",
				"joinToTarget filter(isNull(SourcePersonDim@NCONST)&&isNull(SourcePersonDim@PRIMARYNAME)&&isNull(SourcePersonDim@BIRTHYEAR)&&isNull(SourcePersonDim@DEATHYEAR)) ~> filter1",
				"select2 keyGenerate(output(RowNum as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"filter1 select(mapColumn(",
				"          NCONST = aggregate1@NCONST,",
				"          PRIMARYNAME = aggregate1@PRIMARYNAME,",
				"          BIRTHYEAR = aggregate1@BIRTHYEAR,",
				"          DEATHYEAR = aggregate1@DEATHYEAR,",
				"          MAXPERSONSK,",
				"          COUNT_ROW",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PERSON_SK as decimal(38,0),",
				"          NCONST as string,",
				"          PRIMARYNAME as string,",
				"          BIRTHYEAR as string,",
				"          DEATHYEAR as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          PERSON_SK = Person_SK,",
				"          NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DATE",
				"     )) ~> sink1"
			]
		}
	}
}