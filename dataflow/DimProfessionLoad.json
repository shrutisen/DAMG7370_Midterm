{
	"name": "DimProfessionLoad",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable3",
						"type": "DatasetReference"
					},
					"name": "sourceProfessions"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable4",
						"type": "DatasetReference"
					},
					"name": "sinkDimProfession"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_JOB_ID as string (\"456\"),",
				"     DI_LOAD_DATE as date (currentDate())",
				"}",
				"source(output(",
				"          PRIMARYPROFESSION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT DISTINCT PRIMARYPROFESSION\\nFROM NAME_BASICS\\n',",
				"     format: 'query') ~> sourceProfessions",
				"sourceProfessions keyGenerate(output(ProfessionSK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(DI_JOB_ID = $DI_JOB_ID,",
				"          DI_LOAD_DATE = $DI_LOAD_DATE) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PROFESSION_SK as string,",
				"          PROFESSION_NAME as string,",
				"          DI_JOB_ID as decimal(10,0),",
				"          DI_JOB_DT as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          PROFESSION_SK = ProfessionSK,",
				"          PROFESSION_NAME = PRIMARYPROFESSION,",
				"          DI_JOB_ID,",
				"          DI_JOB_DT = DI_LOAD_DATE",
				"     )) ~> sinkDimProfession"
			]
		}
	}
}