{
	"name": "Dim_Genre_Load",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DimGenreSnowflake",
						"type": "DatasetReference"
					},
					"name": "LoadfromSnowflake"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable3",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "SurrogateKey"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Job_ID as string ('10'),",
				"     parameter1 as date (currentDate())",
				"}",
				"source(output(",
				"          GENRES as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT DISTINCT GENRES FROM DAMG_IMDB.TITLE_BASICS',",
				"     format: 'query') ~> LoadfromSnowflake",
				"LoadfromSnowflake keyGenerate(output(Genre_SK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SurrogateKey",
				"SurrogateKey derive(DI_Job_ID = $Job_ID,",
				"          DI_Load_Date = $parameter1) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          GENRE_SK as string,",
				"          GENRENAME as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          GENRE_SK = Genre_SK,",
				"          GENRENAME = GENRES,",
				"          DI_JOB_ID = DI_Job_ID,",
				"          DI_LOAD_DATE = DI_Load_Date",
				"     )) ~> sink1"
			]
		}
	}
}