							{
								"source": {
									"name": "DI_JOB_ID",
									"type": "String"
								},
								"sink": {
									"name": "DI_JOB_ID",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "DI_LOAD_DATE",
									"type": "String"
								},
								"sink": {
									"name": "DI_LOAD_DATE",
									"type": "DateTime"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "TitleRatingsParquet",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "TitleRatingsSnowflake",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}